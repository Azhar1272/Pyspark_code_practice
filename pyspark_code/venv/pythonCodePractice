# s = "abcefabcdefghkj"
# out = output(s)
# print(out)


def output(input):
    dic = {}
    cnt = 0
    lst = []
    for i in range(len(input)):
        lst.clear()
        lst =  input[i]
        for x in lst:
            if x in dic.keys():
                dic[x] += len(lst)-1
            else:
                dic.update({x:len(lst)-1})

    return dic

fl = [[1,2,3], [2,3,1,4], [4,3]]
print(output(fl))



